{
    "name": "imabug/pcxmcutils",
    "description": "PCXMC Utilities",
    "keywords": ["laravel zero", "console", "cli", "PCXMC"],
    "homepage": "https://github.com/imabug/PCXMCUtils",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "support": {
        "email": "eugenemah@gmail.com",
        "issues": "https://github.com/imabug/PCXMCUtils/issues",
        "source": "https://github.com/imabug/PCXMCUtils",
        "wiki": "https://github.com/imabug/PCXMCUtils/wiki",
        "forum": "https://github.com/imabug/PCXMCUtils/discussions"
    },
    "authors": [
        {
            "name": "Eugene Mah",
            "email": "eugenemah@gmail.com",
            "homepage": "https://v16.imablog.net/"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.4.4",
        "illuminate/database": "^9.28",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^9.1.3",
        "nunomaduro/laravel-console-menu": "^3.3",
        "nunomaduro/laravel-desktop-notifier": "^2.6",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["pcxmcutils"]
}
