{
    "name": "imabug/pcxmcutils",
    "description": "PCXMC Utilities",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "PCXMC"],
    "homepage": "https://v16.imablog.net/",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/imabug/PCXMCUtils/issues",
        "source": "https://github.com/imabug/PCXMCUtils"
    },
    "authors": [
        {
            "name": "Eugene Mah",
            "email": "eugenemah@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^3.0",
        "illuminate/database": "^8.0",
        "illuminate/log": "^8.0",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^8.4",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/laravel-desktop-notifier": "^2.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["pcxmcutils"]
}
